{
	"Logging": {
		"LogLevel": {
			"Default": "Critical",
			"System": "Critical",
			"Microsoft": "Critical",
			"Microsoft.Hosting.Lifetime": "Critical"
		},
		"Debug": {
			"IncludeScopes": true,
			"LogLevel": {
				"Default": "Critical",
				"System": "Critical",
				"Microsoft": "Critical",
				"Microsoft.Hosting.Lifetime": "Critical"
			}
		},
		"Console": {
			"IncludeScopes": true,
			"LogLevel": {
				"Default": "Trace",
				"System": "Error",
				"Microsoft": "Error",
				"Microsoft.Hosting.Lifetime": "Error"
			},
			"FormatterName": "simple",
			"FormatterOptions": {
				"IncludeScopes": true,
				"UseUtcTimestamp": true,
				"TimestampFormat": "MM/dd/yyyy HH:mm:ss ",
				"SingleLine": false,
				"ColorBehavior": "Enabled"
			}
		}
	}
}